{
  "name": "gatsby-source-shopify-storefront",
  "version": "0.1.5",
  "description":
    "Gatsby source plugin for building websites using product data from the Shopify Storefront API",
  "keywords": ["gatsby", "gatsby-plugin", "shopify", "graphql"],
  "author": "Jeff Pamer <jeff@mediumdivision.com>",
  "license": "MIT",
  "bundleDependencies": false,
  "dependencies": {
    "graphql-client": "^2.0.0",
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "cross-env": "^5.0.5"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "watch": "babel -w src --out-dir . --ignore __tests__",
    "prepare": "cross-env NODE_ENV=production npm run build"
  }
}
